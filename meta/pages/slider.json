{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "style": {
            "display": "inline",
            "float": "none"
          },
          "text": "Slider",
          "type": "heading"
        },
        {
          "icon": "",
          "name": "link0",
          "path": "https://github.com/akorchev/angular.radzen.com/blob/master/meta/pages/slider.json",
          "text": "[source code]",
          "type": "link"
        },
        {
          "name": "heading0",
          "size": "H4",
          "text": "This page demonstrates Slider component.",
          "type": "heading"
        },
        {
          "components": [
            {
              "components": [
                {
                  "class": "col-md-6",
                  "components": [
                    {
                      "name": "heading2",
                      "size": "H3",
                      "text": "Slider from 0 to 100",
                      "type": "heading"
                    },
                    {
                      "events": {
                        "change": [
                          {
                            "code": "${events.push('Slider from 0 to 100 Change: ' + JSON.stringify(event))}",
                            "type": "execute"
                          }
                        ]
                      },
                      "name": "slider0",
                      "type": "slider",
                      "value": ""
                    },
                    {
                      "name": "heading3",
                      "size": "H3",
                      "style": {
                        "marginTop": "20px"
                      },
                      "text": "Slider with Step 10",
                      "type": "heading"
                    },
                    {
                      "events": {
                        "change": [
                          {
                            "code": "${events.push('Slider with Step 10 Change: ' + JSON.stringify(event))}",
                            "type": "execute"
                          }
                        ]
                      },
                      "name": "slider1",
                      "range": false,
                      "step": 10,
                      "type": "slider",
                      "value": ""
                    },
                    {
                      "name": "heading4",
                      "size": "H3",
                      "style": {
                        "marginTop": "20px"
                      },
                      "text": "Range Slider",
                      "type": "heading"
                    },
                    {
                      "events": {
                        "change": []
                      },
                      "name": "slider2",
                      "range": true,
                      "step": 1,
                      "type": "slider",
                      "value": "${range}"
                    }
                  ],
                  "name": "col0",
                  "type": "col"
                },
                {
                  "class": "col-md-6",
                  "components": [
                    {
                      "name": "heading1",
                      "size": "H3",
                      "text": "Events",
                      "type": "heading"
                    },
                    {
                      "components": [
                        {
                          "content": "<div style=\"overflow:auto;height:100%\">\n  <div *ngFor=\"let event of events?.reverse()\">\n      {{ event }}\n  </div>\n</div>",
                          "name": "html0",
                          "type": "html"
                        }
                      ],
                      "name": "card1",
                      "style": {
                        "height": "500px"
                      },
                      "type": "card",
                      "visible": true
                    }
                  ],
                  "name": "col1",
                  "type": "col"
                }
              ],
              "name": "row0",
              "type": "row"
            }
          ],
          "name": "card0",
          "style": {
            "height": "600px"
          },
          "type": "card",
          "visible": true
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "name": "events",
        "type": "set",
        "value": "[]"
      },
      {
        "name": "range",
        "type": "set",
        "value": "[20,80]"
      }
    ]
  },
  "layout": "Main",
  "name": "Slider"
}